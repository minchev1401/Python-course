print("""
      Sum of two numbers:""")
def sum():
    num1 = float(input("enter first  num: "))
    num2 = float(input("enter second num: "))
    result = num1 + num2
    return(result)
result = sum()

print(result)


print("""
      Minus of two numbers:""")
def minus():
    num1 = float(input("enter first  num: "))
    num2 = float(input("enter second num: "))
    result = num1 - num2
    return(result)
result = minus()

print(result)


print("""
      Multiple two numbers: """)
def multiple():
    num1 = float(input(" enter first  num: "))
    num2 = float(input(" enter second num: "))
    result = num1 * num2
    return(result)
result= multiple()

print(result)


print("""
      Divide two numbers: """)
def divide():
    num1 = float(input(" Enter first num: "))
    num2 = float(input("Enter second num: "))
    result = num1 / num2
    return(result)
result = divide()

print(result)

print("""
      Find percent from whole number""")
def percentage():
    part = float(input(" enter % we looking for: "))
    whole = float(input("enter whole num: "))
    percent = ((part * whole) / 100)
    return(percent)
percent = percentage()
